{
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.addMissingImports": "explicit"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.rulers": [80],
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
  }
}
